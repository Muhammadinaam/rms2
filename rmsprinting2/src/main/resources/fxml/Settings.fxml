<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainLayout" prefHeight="497.0" prefWidth="583.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.SettingsController">
    <children>
        <TitledPane animated="false" layoutX="10.0" layoutY="10.0" text="Printrs">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="289.0" prefWidth="498.0">
                    <children>
                        <Label layoutX="14.0" layoutY="14.0" text="POS Printer" />
                        <ComboBox fx:id="posPrinter" layoutX="138.0" layoutY="9.0" prefHeight="26.0" prefWidth="196.0" />
                        <Label layoutX="14.0" layoutY="82.0" text="Kitchen Printer 1" />
                        <Label layoutX="171.0" layoutY="59.0" text="Name" />
                        <Label layoutX="293.0" layoutY="59.0" text="Printer" />
                        <TextField fx:id="kitchen1Name" layoutX="138.0" layoutY="77.0" prefHeight="26.0" prefWidth="110.0" />
                        <ComboBox fx:id="kitchen1Printer" layoutX="262.0" layoutY="77.0" prefHeight="26.0" prefWidth="208.0" />
                        <Label layoutX="14.0" layoutY="119.0" text="Kitchen Printer 2" />
                        <TextField fx:id="kitchen2Name" layoutX="138.0" layoutY="114.0" prefHeight="26.0" prefWidth="110.0" />
                        <ComboBox fx:id="kitchen2Printer" layoutX="262.0" layoutY="114.0" prefHeight="26.0" prefWidth="208.0" />
                        <Label layoutX="14.0" layoutY="158.0" text="Kitchen Printer 3" />
                        <TextField fx:id="kitchen3Name" layoutX="138.0" layoutY="153.0" prefHeight="26.0" prefWidth="110.0" />
                        <ComboBox fx:id="kitchen3Printer" layoutX="262.0" layoutY="153.0" prefHeight="26.0" prefWidth="208.0" />
                        <Label layoutX="14.0" layoutY="197.0" text="Kitchen Printer 4" />
                        <TextField fx:id="kitchen4Name" layoutX="138.0" layoutY="192.0" prefHeight="26.0" prefWidth="110.0" />
                        <ComboBox fx:id="kitchen4Printer" layoutX="262.0" layoutY="192.0" prefHeight="26.0" prefWidth="208.0" />
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
        <TitledPane animated="false" text="Other Settings">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="109.0" prefWidth="498.0">
                    <children>
                        <Label layoutX="14.0" layoutY="20.0" text="POS API URL" />
                        <TextField fx:id="posApiUrl" layoutX="130.0" layoutY="15.0" prefHeight="26.0" prefWidth="398.0" />
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
        <HBox>
            <children>
                <Region HBox.hgrow="ALWAYS" />
                <Button fx:id="btnSave" mnemonicParsing="false" onAction="#btnSaveClicked" text="Save" />
            </children>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
